{
    "extends": ["react-app"],
    "plugins": ["react", "import"],
    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    },
    "rules" : {
        /*"no-unused-vars" : 2,
        "no-undef" : 2*/
        //"react/prop-types": 0
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "allowImportExportEverywhere": true,
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
